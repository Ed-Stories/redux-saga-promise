{
  "name": "@adobe/redux-saga-promise",
  "version": "1.1.0",
  "description": "Create actions that return promises, which are resolved/rejected by a redux saga",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx babel src --out-dir dist",
    "start": "npx babel src --watch --out-dir dist",
    "test": "npm run build && ava",
    "lint": "npx eslint src test"
  },
  "keywords": [
    "redux",
    "saga",
    "action",
    "promise",
    "promises",
    "resolve",
    "reject"
  ],
  "author": "Ronen Barzel <ronen@barzel.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "ava": "^3.8.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-redux-saga": "^1.1.3",
    "eslint-plugin-standard": "^4.0.1",
    "esm": "^3.2.25"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "lodash": "^4.17.15",
    "redux-actions": "^2.6.5",
    "redux-saga": "^1.1.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adobe/redux-saga-promise.git"
  },
  "bugs": {
    "url": "https://github.com/adobe/redux-saga-promise/issues"
  },
  "homepage": "https://github.com/adobe/redux-saga-promise#readme",
  "ava": {
    "require": [
      "esm"
    ]
  }
}
